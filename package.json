{
  "name": "twelve-x-twelve.press",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "docker-build-tar": "cross-env DOCKER_BUILDKIT=1 docker build -t recovery-rocks-backend:latest . && docker save -o tars/recovery-rocks-backend.tar recovery-rocks-backend:latest",
    "scp-tar": "scp tars/recovery-rocks-backend.tar root@89.111.172.66:/tmp/docker-images/",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload-graphql generate:schema",
    "migrate:create": "payload migrate:create",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "yarn run test:int && yarn run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" yarn playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@faceless-ui/modal": "3.0.0",
    "@faceless-ui/scroll-info": "2.0.0",
    "@innovixx/payload-color-picker-field": "2.0.2",
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/payload-cloud": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/translations": "3.69.0",
    "@payloadcms/ui": "3.69.0",
    "@vvo/tzdb": "^6.198.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "libphonenumber-js": "^1.12.33",
    "next": "15.4.10",
    "payload": "3.69.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@payloadcms/graphql": "^3.69.0",
    "@playwright/test": "1.54.1",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "~19.2.0",
    "@types/react-dom": "~19.2.0",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite": "^7",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "packageManager": "yarn@4.12.0"
}
